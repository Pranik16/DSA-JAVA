class Solution {
    public int[] productExceptSelf(int[] nums) {
        
        int lprod=1;
        int rprod=1;
        
        int i,j;
        int [] n= new int[nums.length];
        n[0]=1;
        for(i=1;i<nums.length;i++){
            n[i]=n[i-1]*nums[i-1];            
          }
        for(j=nums.length-1;j>=0;j--){
            n[j]=n[j]*rprod;
            rprod= rprod*nums[j];
        }
    
    return n;    
    }
}
