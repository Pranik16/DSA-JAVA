class Solution {
    public double findMaxAverage(int[] nums, int k) {
        
        double csum=0;
        double msum=0;

        for(int i=0;i<k;i++){
                csum+=nums[i];
            }
        msum=csum;
        for(int j=k;j<nums.length;j++){
            csum-=nums[j-k];
            csum+=nums[j];
            msum=Math.max(msum,csum);
        }
           
    return (msum/k);
}
}


SLIDING WINDOW TECHNIQUE
