class Solution {
    public int[] sortedSquares(int[] nums) {
        for(int i=0; i<nums.length ; i++)
        {
            nums[i]= nums[i]*nums[i];
        }
    for(int k=0; k<nums.length-1 ; k++)
    {
        for(int i=0; i<nums.length-1 ; i++)
        {
            
            if(nums[i]>nums[i+1])
            {
                int temp=nums[i];
                nums[i]=nums[i+1];
                nums[i+1]=temp;
            }
        }
    }
        return nums;
    }
}
