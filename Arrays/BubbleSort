class Solution {
    public void sortColors(int[] nums) {
        int i,j;
        for(i=1;i<nums.length;i++)
        {
        int key=nums[i];
        j=i-1;
        while(j>=0 && nums[j]>key){
            nums[j+1]=nums[j];
            j--;
        }
        nums[j+1]=key;
        
    }
    System.out.print(nums);
}
}
