class Solution {
    public char[] reverseString(char[] s) {
        int n = s.length;
        char[] str= new char[n];
        int j=0;
        for(int i=n-1; i>=0;i--)
        {
            str[j] = s[i];
            j++;
        }
        for (int i = 0; i < n; i++) {
            s[i] = str[i];
            }
        return s;
        }
}
