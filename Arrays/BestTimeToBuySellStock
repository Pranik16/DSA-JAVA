class Solution {
    public int maxProfit(int[] prices) {

        int buy=prices[0];
        int mprofit=0;
        int profit=0;
        for(int i=1;i<prices.length;i++){
            if(buy>prices[i])
            buy=prices[i];
            else
            profit=prices[i]-buy;
            mprofit=Math.max(profit,mprofit);
        }
    return mprofit;
    }
}
