class Solution {
    public int maximumWealth(int[][] accounts) {
        int cwealth=0;
        int wealth;
        int i,j;
        for(i=0 ; i<accounts.length; i++)
        {
            wealth=0;

            for(j=0 ; j<accounts[i].length; j++)
            {
                wealth += accounts[i][j];
            }
        
        if(wealth>cwealth){
            cwealth=wealth;
            }
        }
        return cwealth;
    }
    
}
